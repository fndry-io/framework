{
    "name": "foundry/core",
    "description": "Foundry Framework.",
    "keywords": ["foundry", "framework", "laravel"],
    "license": "MIT",
    "type": "foundry-core",
    "require": {
        "php": "^7.1",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Foundry\\Core\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Foundry\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foundry\\Core\\FoundryServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "scripts": {

    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
