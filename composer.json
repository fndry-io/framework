{
  "name": "foundry/core",
  "description": "Foundry Framework.",
  "keywords": [
    "foundry",
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "foundry-core",
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.8.*",
    "laravel/tinker": "^1.0",
    "phpunit/phpunit": "^7.0",
    "gedmo/doctrine-extensions": "^2.4",
    "laravel-doctrine/extensions": "1.0.*",
    "laravel-doctrine/migrations": "^1.2",
    "laravel-doctrine/orm": "1.4.*",
    "ramsey/uuid-doctrine": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Foundry\\Core\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Foundry\\Core\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Foundry\\Core\\FoundryServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "scripts": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
