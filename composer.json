{
    "name": "foundry/core",
    "description": "Foundry Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/fndry-io/laravel-plugins.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "nwidart/laravel-modules":"dev-features"
    },
    "require-dev": {
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Foundry\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Foundry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {

    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
